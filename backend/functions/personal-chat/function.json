{
  "function": "HttpTrigger",
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": ["get", "post"],
      "route": "personal-chat/{action}/{chatId?}"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    }
  ]
}